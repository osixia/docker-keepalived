{
  "def_queue": [
    "--disable-systemd",
    "--disable-dynamic-linking",
    "--prefix=/usr",
    "--exec-prefix=/usr",
    "--bindir=/usr/bin",
    "--sbindir=/usr/sbin",
    "--sysconfdir=/etc",
    "--datadir=/usr/share",
    "--localstatedir=/var",
    "--mandir=/usr/share/man"
  ],
  "mapping": [
    {
      "argument": "--enable-dbus",
      "dest": "enable_dbus",
      "type": "int",
      "default": 0,
      "help": "Compile with dbus support.",
      "related_arguments": []
    },
    {
      "argument": "--disable-libipset",
      "dest": "disable_libipset",
      "type": "int",
      "default": 0,
      "help": "Compile without libipset.",
      "related_arguments": []
    },
    {
      "argument": "--disable-iptables",
      "dest": "disable_iptables",
      "type": "int",
      "default": 0,
      "help": "Compile without iptables support.",
      "related_arguments": []
    },
    {
      "argument": "--disable-nftables",
      "dest": "disable_nftables",
      "type": "int",
      "default": 0,
      "help": "Compile without nftables support.",
      "related_arguments": []
    },
    {
      "argument": "--enable-snmp-vrrp",
      "dest": "enable_snmp_vrrp",
      "type": "int",
      "default": 0,
      "help": "Compile with snmp vrrp support.",
      "related_arguments": ["--enable-snmp-rfc"]
    }
  ]
}
